1480. Running Sum of 1d Array

class Solution {
    public int[] runningSum(int[] nums) {
        int runningSum[]=new int[nums.length];
        int sum=nums[0];
        for(int i=0;i<nums.length-1;i++){
          runningSum[i]=sum;
          sum+=nums[i+1];
        }
        runningSum[nums.length-1]=sum;
        return runningSum;
    }
}
